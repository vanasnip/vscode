{
  "window.zoomLevel": 0,
  "workbench.sideBar.location": "left",
  "workbench.iconTheme": "Monokai Pro Icons",
  "editor.minimap.enabled": false,
  "workbench.statusBar.visible": true,
  "workbench.editor.showTabs": true,
  "liveServer.settings.port": 5560,
  "workbench.colorCustomizations": {
    "terminal.foreground": "#fdfae8",
    "terminal.background": "#202020",
  },
  "javascript.implicitProjectConfig.checkJs": true,
  //   "typescript.validate.enable": true,
  // "javascript.validate.enable": true,
  "update.showReleaseNotes": false,
  "workbench.startupEditor": "none",
  "workbench.activityBar.visible": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<S-space>": true
  },
  "editor.fontFamily": "'DejaVu Sans Mono', Hack, Dank Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic
          "meta.class meta.method.declaration meta.var.expr storage.type.js",
          "source.go keyword.const.go",
          "source.go keyword.var.go",
          "storage",
          "storage.type",
          "variable",
          "tag.decorator.js entity.name.tag.js",
          "variable.other.object.property",
          "meta.var.expr",
          "comment",
          "emphasis",
          "entity.name.method.js",
          "entity.name.class.js",
          "entity.name.tag.doctype",
          "entity.name.React",
          "entity.other.attribute-name",
          "entity.other.attribute-name.tag.jade",
          "entity.other.attribute-name.tag.pug",
          "keyword",
          "keyword.control",
          "keyword.operator.comparison",
          "keyword.control.flow.js",
          "keyword.control.flow.ts",
          "keyword.control.flow.tsx",
          "keyword.control.ruby",
          "keyword.control.module.ruby",
          "keyword.control.class.ruby",
          "keyword.control.def.ruby",
          "keyword.control.loop.js",
          "keyword.control.loop.ts",
          "keyword.control.import.js",
          "keyword.control.import.ts",
          "keyword.control.import.tsx",
          "keyword.control.from.js",
          "keyword.control.from.ts",
          "keyword.control.from.tsx",
          "keyword.operator.expression.delete",
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.cast",
          "keyword.operator.relational",
          "keyword.operator.sizeof",
          "keyword.operator.logical.python",
          "italic",
          "markup.italic",
          "markup.quote",
          "markup.changed",
          "markup.italic.markdown",
          "markup.quote.markdown",
          "markup.deleted.diff",
          "markup.inserted.diff",
          "meta.delimiter.period",
          "meta.diff.header.git",
          "meta.diff.header.from-file",
          "meta.diff.header.to-file",
          "meta.tag.sgml.doctype",
          "meta.decorator punctuation.decorator",
          "meta.selector",
          "punctuation.accessor",
          "punctuation.definition.comment",
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end",
          // "punctuation.section.embedded",//braces brackets ets
          "quote",
          "source.js constant.other.object.key.js string.unquoted.label.js",
          "source.go keyword.package.go",
          "source.go keyword.import.go",
          "source.go keyword.function.go",
          "source.go keyword.type.go",
          "source.go keyword.struct.go",
          "source.go keyword.interface.go",
          "source.go keyword.map.go",
          "source.go keyword.channel.go",
          "source.go keyword.control.go",
          "storage.modifier",
          "storage.type.property.js",
          "storage.type.property.ts",
          "storage.type.property.tsx",
          "tag.decorator.js",
          "text.html.basic entity.other.attribute-name.html",
          "text.html.basic entity.other.attribute-name",
          "variable.language",
          "javascript"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.function",
        "settings": {}
      },
      {
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#ffc402",
        }
      },
      {
        "scope": "variable.other.readwrite",
        "settings": {
          "foreground": "#41bd41",
        }
      },
      {
        "scope": "variable",
        "settings": {
          "foreground": "#77e3f3"
        }
      },
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#f62929"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#b0bf9c",
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#fe89ec",
        }
      },
      {
        "scope": [
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end"
        ],
        "settings": {
          "foreground": "#ff0000",
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#bef988"
          // conditionals
        }
      },
      {
        "scope": "string.unquoted, constant.other.object.key",
        "settings": {
          "foreground": "#009df2",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "workbench.fontAliasing": "antialiased",
